                            @{
                                List<Omni.Agence.DAL.ContratLocation> ListContrat = ViewBag.ListOP;
                            }
                            @{ 
                                List<Omni.Agence.DAL.ContratLocation> listC = ViewBag.ListContrat;
                                
                            }
