<script language=JavaScript>

    function date() {

        var today = new Date();
        var date_heure = "";

        h = today.getHours();
        m = today.getMinutes();
        s = today.getSeconds();


        if (h < 10)
        { h = '0' + h; }
        if (m < 10)
        { m = '0' + m; }
        if (s < 10)
        { s = '0' + s; }
        //date_heure = ''+h+'h'+m+'m'+s+'';
        date_heure = h;
        date_min = m;
        date_sec = s;

        document.getElementById('heur').innerHTML = date_heure;
        document.getElementById('min').innerHTML = date_min;
        document.getElementById('sec').innerHTML = date_sec;
    }
    setInterval("date()", 1000);

</script>
