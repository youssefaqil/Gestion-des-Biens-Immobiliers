<h3 class="page-title">
    Locataires <small>Détails</small>
</h3>
<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <i class="fa fa-home"></i>
            <a href="@Url.Action("Index","Home")">Accueil</a>
            <i class="fa fa-angle-right"></i>
        </li>
        <li>
            <i class="fa fa-user"></i><a href="@Url.Action("Index")">Locataire</a>
            <i class="fa fa-angle-right"></i>
        </li>
        @if (Model.rc.IsEmpty())
