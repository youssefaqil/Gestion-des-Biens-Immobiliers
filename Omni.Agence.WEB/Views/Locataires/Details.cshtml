<div class="row">
    <div class="col-md-12">
        <div class="portlet box green-haze">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-edit"></i>Tous les Contrats du @Html.DisplayFor(model => model.nom) @Html.DisplayFor(model => model.prenom)
                </div>
                @*<div class="tools">
                        <a href="javascript:;" class="collapse">
                        </a>

                    </div>*@
            </div>
            <div class="portlet-body">
                <div class="table-toolbar">
                    <div class="row">
                        <div class="col-md-6">
                            @using System.Linq;
                            @using System.Data.Entity;


                            @{
                                Omni.Agence.DAL.AgenceEntities db = new Omni.Agence.DAL.AgenceEntities();
                                var contratLocations = db.ContratLocations.Include(c => c.Local).Include(c => c.Personne).Where(p => p.CodePers == Model.CodePers);

                                List<Omni.Agence.DAL.ContratLocation> contrats = contratLocations.ToList();

                            }

                        </div>

                    </div>
                </div>

                <table class="table table-striped table-bordered table-hover" id="sample_3">
                    <thead>
                        <tr>
                            <th>
                                Usage
                            </th>
                            <th>
                                Local
                            </th>

                            <th>
                                Début
                            </th>
                            <th>
                                Fin
                            </th>

                            <th>
                                Loyer de Base
                            </th>
                            <th>
                                Loyer Net
                            </th>
                            <th>
                                Prochaine Aug.
                            </th>
                            <th>
                                Aug. %
                            </th>
                            <th>Statut</th>
                            <th></th>
                        </tr>

                    </thead>
                    <tbody>
                        @foreach (var item in contrats)
                        {
                            <tr>
                                <td>
                                    @{

                            if (item.Usage == 0)
                            {
                                ViewBag.Usage = "Habitation";
                            }
                            else if (item.Usage == 1)
                            {
                                ViewBag.Usage = "Commercial";
                            }
                            else
                            {
                                ViewBag.Usage = "";
                            }
                                    }
                                    @ViewBag.Usage
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Local.TitreLocal)
                                </td>

                                <td>
                                    @item.DateJuissance.Value.ToString("dd/MM/yyyy")
                                </td>

                                <td>
                                    @if (item.DateFinContrat.HasValue)
                                    {
                                        item.DateFinContrat.Value.ToString("dd/MM/yyyy");
                                    }
                                   
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.LoyerDebase)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LoyerNet)
                                </td>
                                <td>
                                    @if ((item.Augementations.Count() != 0) && item.Augementations.Where(d => d.dateAug > DateTime.Now).Count() != 0)
                                    {

                                        @item.Augementations.Where(d => d.dateAug > DateTime.Now).OrderBy(d => d.dateAug).First().dateAug.Value.ToString("dd/MM/yyyy")
                                        @*@Html.DisplayFor(modelItem => item.Augementations.Where(d=>d.dateAug<DateTime.Now).OrderBy()*@
                                    }
                                </td>
                                <td>
                                    @if ((item.Augementations.Count() != 0) && item.Augementations.Where(d => d.dateAug > DateTime.Now).Count() != 0)
                                    {
                                        @Html.DisplayFor(modelItem => item.Augementations.Where(d => d.dateAug > DateTime.Now).OrderBy(d => d.dateAug).First().pcAug)
                                    }
                                </td>
                                <td>
                                    @{
                            if (Html.DisplayFor(modelItem => item.isClosed).ToString() == "1")
                            {
                                ViewBag.dispo = "success";
                                ViewBag.ico = "fa-check-square";
                                ViewBag.chaine = "En cours";
                                //ViewBag.SuppLocal = Html.ActionLink("Supprimer", "Delete", new { id = item.CodeLocal });
                                //ViewBag.SuppLocal = Url.Action("Delete", new { id = item.CodePers });

                            }
                            else if (Html.DisplayFor(modelItem => item.isClosed).ToString() == "0")
                            {
                                ViewBag.dispo = "danger";
                                ViewBag.ico = "fa-minus-square";
                                ViewBag.chaine = "Fermé";
                                //ViewBag.SuppLocal = "";

                            }

                                    }<center>
                                        <span class="label label-sm label-@ViewBag.dispo">
                                            @ViewBag.chaine
                                        </span>
                                    </center>
                                </td>

                                <td>
                                    <a href="@Url.Action("DetailsContrat", new { id = item.CodeContrat })" class="btn default btn-xs green-haze tooltips" data-placement="bottom" data-original-title="Détails Contrat">
                                        <i class="fa fa-pencil"></i>
                                    </a>

                                    <a href="@Url.Action("CreatePL", new { id = item.CodeContrat })" class="btn default btn-xs blue-chambray tooltips" data-placement="bottom" data-original-title="Paiement">
                                        <i class="fa fa-money"></i>
                                    </a>

                                    @*@Html.ActionLink("Paiement", "PaiementLoc", new { id = item.CodeContrat })*@

                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
