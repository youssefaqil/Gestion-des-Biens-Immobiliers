            <li>
                <a href="@Url.Action("Details", new {id=Model.CodePers })">@Html.DisplayFor(model => model.nom) @Html.DisplayFor(model => model.prenom)</a>
                <i class="fa fa-angle-right"></i>
            </li>
        }
