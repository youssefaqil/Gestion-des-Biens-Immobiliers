                        <tr>
                            <th>
                                Superficie
                            </th>
                            <th>
                                Pièces
                            </th>

                            <th>
                                Titre
                            </th>



                            <th>
                                Dérnier contrat
                            </th>
                            <th>
                                Date fin contrat
                            </th>
                            <th>
                                Montant net dernier loyer
                            </th>

                            <th>
                                service%
                            </th>
                            <th>Disponibilité</th>

                            <th></th>
                        </tr>

                    </thead>
                    <tbody>

                        @using Omni.Agence.DAL;

                        @foreach (var item in (List<Local>)ViewBag.ListLocalImmeuble)
                        {
                            <tr>
                                <td>

                                    @Html.DisplayFor(modelItem => item.SuperficieLocal)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.NbrPieceLocal)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.TitreLocal)
                                </td>
                                <td>
                                    @if ((item.ContratLocations.Count() > 0))
                                    {
                                        @item.ContratLocations.OrderBy(d => d.DateJuissance).First().DateJuissance.Value.ToString("dd/MM/yyyy");
                                    }
                                </td>
                                <td>
                                    @if ((item.ContratLocations.Count() > 0))
                                    {

                                        @item.ContratLocations.OrderBy(d => d.DateJuissance).First().DateFinContrat.Value.ToString("dd/MM/yyyy");
                                    }
                                </td>

                                <td>
                                    @if ((item.ContratLocations.Count() != 0))
                                    {
                                        @Html.DisplayFor(modelItem => item.ContratLocations.OrderBy(d => d.DateJuissance).First().PaiementLoyers.OrderBy(d => d.Operation.dateOp).First().Operation.montant.Value);
                                    }
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ServiceLocal)
                                </td>

                                <td>
                                    @{
                            if (Html.DisplayFor(modelItem => item.Staut).ToString() == "0")
                            {
                                ViewBag.dispo = "success";
                                ViewBag.ico = "fa-check-square";
                                ViewBag.chaine = "Oui ";
                                ViewBag.SuppLocal = Url.Action("Delete", new { id = item.CodeLocal });



                            }
                            else if (Html.DisplayFor(modelItem => item.Staut).ToString() == "1")
                            {
                                ViewBag.dispo = "danger";
                                ViewBag.ico = "fa-minus-square";
                                ViewBag.chaine = "Non";
                                ViewBag.SuppLocal = "";

                            }

                                    }<center>
                                        <span class="label label-sm label-@ViewBag.dispo">
                                            <i class="fa @ViewBag.ico"></i> @ViewBag.chaine
                                        </span>
                                    </center>
                                </td>
                                <td>
                                    <a href="#@item.CodeLocal" class="btn default btn-xs purple-intense  tooltips" data-toggle="modal" data-placement="left" data-original-title="Détails Appartement ">
                                        <i class="fa fa-edit"></i>
                                    </a>
                                    <div id="@item.CodeLocal" class="modal fade" tabindex="-1" aria-hidden="true">
                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                                    <h4 class="modal-title">Nouvel appartement</h4>
                                                </div>
                                                <div class="modal-body">
                                                    <h3 class="form-section">Informations</h3>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label col-md-5">Nom:</label>
                                                                <div class="col-md-5">

                                                                    @Html.DisplayFor(modelItem => item.TitreLocal)
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!--/span-->
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label col-md-5">Superficie:</label>
                                                                <div class="col-md-5">
                                                                    @Html.DisplayFor(modelItem => item.SuperficieLocal)
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!--/span-->
                                                    </div>
                                                    <!--/row-->
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label col-md-5">Pièces:</label>
                                                                <div class="col-md-5">
                                                                    @Html.DisplayFor(modelItem => item.NbrPieceLocal)
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!--/span-->


                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label col-md-5">Chambres:</label>
                                                                <div class="col-md-5">
                                                                    @Html.DisplayFor(modelItem => item.NbrChambreLocal)
                                                                </div>
                                                            </div>

                                                        </div>
                                                        <!--/span-->
                                                    </div>
                                                    <!--/row-->
                                                    <div class="row">

                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label col-md-5">Prix:</label>
                                                                <div class="col-md-5">
                                                                    @Html.DisplayFor(modelItem => item.PrixRefLocationLocal)
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label class="control-label col-md-5">Service:</label>
                                                                <div class="col-md-5">
                                                                    @Html.DisplayFor(modelItem => item.ServiceLocal)
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!--/span-->
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label class="control-label col-md-5">Description:</label>
                                                                <div class="col-md-8">

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-9">
                                                            @Html.DisplayFor(modelItem => item.DescrLocal)
                                                        </div>
                                                        </div>
                                                    <div class="row">

                                                        <!--/span-->
                                                        <!--/span-->
                                                    </div>

                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" data-dismiss="modal" class="btn default">Close</button>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </td>

                            </tr>
                        }
                    </tbody>


                </table>


            </div>


        </div>
    </div>
</div>
